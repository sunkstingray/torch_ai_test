{
  "name": "torch_ai_test",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' server.js\" \"yarn client\"",
    "client": "cd frontend && yarn build:dev",
    "install": "cd frontend && yarn install",
    "build": "cd frontend && yarn build",
    "heroku-postbuild": "yarn build",
    "lint": "eslint \"**/*.js\" --quiet && cd frontend && yarn lint",
    "fix": "eslint \"**/*.js\" --fix .",
    "test": "yarn lint && NODE_ENV=test mocha -u tdd --reporter spec --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunkstingray/torch_ai_test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunkstingray/torch_ai_test/issues"
  },
  "homepage": "https://github.com/sunkstingray/torch_ai_test#readme",
  "dependencies": {
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "puppeteer": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "nodemon": "^1.18.6"
  }
}
